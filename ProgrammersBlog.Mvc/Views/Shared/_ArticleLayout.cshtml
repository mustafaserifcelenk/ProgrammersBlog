<!DOCTYPE HTML>
<html lang="tr">
<head>
    <title>@ViewBag.Title | Kısık Ses </title>
    <meta charset="utf-8" />
    <meta name="description" content="@ViewBag.Description" />
    <meta name="author" content="Kısık Ses" />
    <meta name="keywords" content="@ViewBag.Tags" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <link href="~/BlogHome/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Striped/assets/css/main.css" />
</head>
<body class="is-preload">

    <!-- Content -->
    @RenderBody()

    <!-- Sidebar -->
    @await Component.InvokeAsync("RightSideBar")


    <!-- Scripts -->
    <script src="~/Striped/assets/js/jquery.min.js"></script>
    <script src="~/Striped/assets/js/browser.min.js"></script>
    <script src="~/Striped/assets/js/breakpoints.min.js"></script>
    <script src="~/Striped/assets/js/util.js"></script>
    <script src="~/Striped/assets/js/main.js"></script>
    <script src="~/BlogHome/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

</body>
</html>